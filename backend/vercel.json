{
    "version": 1,
    "name": "SocialNetwork",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "Methods": ["GET", "POST", "PUT", "DELETE"],
            "header": {
                "Access-Control_Allow-Origin": "*"
            }
        }
    ]
}